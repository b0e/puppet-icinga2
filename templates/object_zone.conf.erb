object Zone "<%= @object_name %>" {
  <%- if @endpoints.length != 0 -%>
    endpoints = [ <%- @endpoints.each do |endpoint| -%> "<%= endpoint %>", <%- end -%>]
  <%- end -%>
  <%- if @parent -%>
	  parent = "<%= @parent -%>"
  <%- end -%>
}
